<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<mappings xmlns="http://dozermapper.github.io/schema/bean-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://dozermapper.github.io/schema/bean-mapping https://dozermapper.github.io/schema/bean-mapping.xsd">
    <configuration>
        <wildcard>false</wildcard>
    </configuration>
    <mapping>
        <class-a>localidadresponse.LocalidadResponse</class-a>
        <class-b>provinciaresponse.ProvinciaResponse</class-b>
        <field>
            <a>localidades</a>
            <b>provincia.localidades</b>
        </field>
    </mapping>
    <mapping>
        <class-a>localidadresponse.Localidade</class-a>
        <class-b>provinciaresponse.Localidade</class-b>
        <field>
            <a>id</a>
            <b>id</b>
        </field>
        <field>
            <a>nombre</a>
            <b>nombre</b>
        </field>
    </mapping>
    <mapping>
        <class-a>org.apache.camel.component.dozer.VariableMapper</class-a>
        <class-b>provinciaresponse.ProvinciaResponse</class-b>
    </mapping>
    <mapping>
        <class-a>org.apache.camel.component.dozer.ExpressionMapper</class-a>
        <class-b>provinciaresponse.ProvinciaResponse</class-b>
        <field custom-converter-id="_expressionMapping" custom-converter-param="simple:\${header.fecha}">
            <a>expression</a>
            <b>fecha</b>
        </field>
        <field custom-converter-id="_expressionMapping" custom-converter-param="simple:\${header.hora}">
            <a>expression</a>
            <b>hora</b>
        </field>
        <field custom-converter-id="_expressionMapping" custom-converter-param="simple:\${property.provinciaId}">
            <a>expression</a>
            <b>provincia.id</b>
        </field>
        <field custom-converter-id="_expressionMapping" custom-converter-param="simple:\${property.provinciaDesc}">
            <a>expression</a>
            <b>provincia.nombre</b>
        </field>
    </mapping>
</mappings>
